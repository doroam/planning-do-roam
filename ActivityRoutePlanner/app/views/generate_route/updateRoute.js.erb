<%route = session[:main_route]
if params[:end]
  point = route.end_point
  type = "end"
else
  point = route.start_point
  type = "start"
end
%>
addMark("<%=point.lat%>","<%=point.long%>","<%=type%>");
var elem = document.getElementById('<%=type%>_id');
elem.innerHTML = "<%= escape_javascript(render(:partial => 'route/routeForm',:locals => {:point => point,:type => type}))%>";
